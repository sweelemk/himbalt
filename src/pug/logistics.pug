extends _layout

block title
	title Index
block content
	.root_wrapper.load-dom
		i.overlay
		.ui-menu
			span.ui-menu__content
				i.ui-menu-line.ui-menu-line-1
				i.ui-menu-line.ui-menu-line-2
				i.ui-menu-line.ui-menu-line-3
		.root_frame
			.root_frame-content
				.pages.inner-page(id="my-scrollbar")
					section.constant-section.inactive.js-constant
						include includes/_constant-section
						.breadcumbs.show
							.breadcumbs-inner
								.breadcrumbs-content
									.breadcrumbs-item
										a(href="/" class="js-link")
											span Logistics
									.breadcrumbs-item
										a(href="/" class="js-link")
											span Home
					section.variable-section
						.logo-container
							- var Index = false
							if(Index)
								.logo
							else
								a(href="/" class="js-link logo")
						include includes/_header
						.loader-container
							.section-scroll-container
								.section-scroll.js-ajx(data-pages="true")
									//- main container
									article.container-content
										section.intro-head
											h1.h1
												span Logistics
										section.section-anchors
											.anchor-container
												ul
													mixin anchors(name, link)
														li.achors-item
															a.anchor-trigger(href="#" + link)
																span #{name}
													+anchors("Base oils", "group-1")
													+anchors("Petrochemicals", "group-2")
													+anchors("Paraffin wax", "group-3")
										section.section.js-section(data-inview-class="push-it")
											.l
												p.lead Himbalt company organizes the transportation and supply of petroleum products, oils and polyethylene around the world: from Europe to Asia, from Australia to America.
										section.section.js-section(data-inview-class="push-it")
											.l
												div.quote Extensive experience and knowledge of all the nuances enable us to realize any wish of the client. For us, nothing is impossible. 	
										section.section.js-section(data-inview-class="push-it")
											.container-image
												.image-element
													img(src="img/image-content/img-2.jpg")
												.text-elements(id="group-1")
													h2.h2 Base oils
													p The logistics tasks include processes regulating the carriage of goods at destination. Transport logistics involves a number of operations, including packaging, insurance, freight forwarding, clearance at customs warehousing. This makes it possible to optimize the costs by selecting the optimum route and prompt registration of supporting documents. 

										section.section.js-section(data-inview-class="push-it")
											div.container
												div.column-100.text-center
													h3.h3.margin-bottom-h3 Packaging solutions <br> delivery of base oils
